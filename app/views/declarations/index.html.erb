<p id="notice"><%= notice %></p>

<h1>Declarations</h1>

<table>
  <thead>
    <tr>
      <th>Ip address</th>
      <th>City</th>
      <th>Country name</th>
      <th>Country code</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Time zone</th>
      <th>Image</th>
      <th>Rawemotiondata</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @declarations.each do |declaration| %>
      <tr>
        <td><%= declaration.ip_address %></td>
        <td><%= declaration.city %></td>
        <td><%= declaration.country_name %></td>
        <td><%= declaration.country_code %></td>
        <td><%= declaration.latitude %></td>
        <td><%= declaration.longitude %></td>
        <td><%= declaration.time_zone %></td>
        <td><%= declaration.image %></td>
        <td><%= declaration.rawemotiondata %></td>
        <td><%= declaration.email %></td>
        <td><%= link_to 'Show', declaration %></td>
        <td><%= link_to 'Edit', edit_declaration_path(declaration) %></td>
        <td><%= link_to 'Destroy', declaration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Declaration', new_declaration_path %>
